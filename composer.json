{
    "name": "prinsfrank/php-validated-properties",
    "description": "Validated properties in PHP8.1 and above using attribute rules",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.2",
        "phpstan/phpstan": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "PrinsFrank\\PhpStrictModels\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PrinsFrank\\PhpStrictModels\\Tests\\": "tests/",
            "PrinsFrank\\PhpStrictModels\\Development\\": "development/"
        }
    },
    "scripts": {
        "fix": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    }
}
