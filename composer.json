{
    "name": "prinsfrank/php-strict-models",
    "description": "Strict models for PHP",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "PrinsFrank\\PhpStrictModels\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PrinsFrank\\PhpStrictModels\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "fix": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    }
}
